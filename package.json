  },
  "devDependencies": {
    "popper.js": "^1.14.4",
    "jquery": "^3.3.1",
    "bootstrap": "^4.1.3",
    "babel-plugin-transform-flow-comments": "^6.7.0",
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-standard": "^3.0.4",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17",
    "vue": "^2.5.17",
    "rimraf": "^2.6.2"
